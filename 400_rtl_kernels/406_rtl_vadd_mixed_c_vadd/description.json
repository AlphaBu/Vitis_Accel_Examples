        "hw_emu"
    ], 
    "host_srcs": "src/host.cpp", 
    "key_concepts": [
        "Mixed Kernels"
    ], 
    "sdx_gui": "false", 
    "includes": [
        {
            "name": "xcl2", 
            "location": "GIT_REPO_DIR/common/includes/xcl2/"
        }
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "os": [
        "Linux"
    ], 
    "example": "Mixed Kernels (RTL & C)", 
    "containers": [
        {
            "accelerators": [
                {
                    "kernel_type": "RTL", 
                    "name": "rtl_vadd"
                }, 
                {
                    "name": "krnl_vadd", 
                    "location": "src/krnl_vadd.cpp"
                }
            ], 
            "name": "vadd"
        }
    ], 
    "output_files": "tmp_kernel_pack* packaged_kernel*"
}