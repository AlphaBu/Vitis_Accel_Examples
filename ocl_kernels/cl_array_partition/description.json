{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example shows how to use array partitioning to improve performance of a kernel"
    ], 
    "name": "Array Partitioning (CL)", 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "array_partition"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Array Partitioning"
    ], 
    "keywords": [
        "xcl_array_partition", 
        "complete"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "matmul", 
                    "location": "src/matmul.cl"
                }, 
                {
                    "name": "matmul_partition", 
                    "location": "src/matmul.cl"
                }
            ], 
            "name": "matmul"
        }
    ], 
    "output_files": "output.png"
}