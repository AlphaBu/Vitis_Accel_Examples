{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example is a simple OpenCL application. It will highlight the basic flow of an OpenCL application."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "helloworld"
        }
    ], 
    "key_concepts": [
        "OpenCL API"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vector_add", 
                    "location": "src/vector_addition.cl"
                }
            ], 
            "name": "vector_addition"
        }
    ], 
    "name": "Hello World (CL)"
}