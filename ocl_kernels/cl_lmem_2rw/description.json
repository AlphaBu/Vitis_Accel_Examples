{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is simple example of vector addition to demonstrate how to utilized both ports of Local Memory."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "2port BRAM Utilization", 
        "two read/write Local Memory"
    ], 
    "keywords": [
        "opencl_unroll_hint(2)"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vadd", 
                    "location": "src/vadd.cl"
                }
            ], 
            "name": "vadd"
        }
    ], 
    "name": "Two Parallel Read/Write on Local Memory (CL)"
}