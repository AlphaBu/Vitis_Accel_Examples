{
    "testinfo": {
        "negative": "true"
    }, 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example discuss the different reasons for errors in OpenCL C++ and how to handle them at runtime."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "errors"
        }
    ], 
    "key_concepts": [
        "OpenCL C++ API", 
        "Error handling"
    ], 
    "keywords": [
        "CL_SUCCESS", 
        "CL_DEVICE_NOT_FOUND", 
        "CL_DEVICE_NOT_AVAILABLE", 
        "CL_INVALID_VALUE", 
        "CL_INVALID_KERNEL_NAME", 
        "CL_INVALID_BUFFER_SIZE"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vector_add", 
                    "location": "src/vector_addition.cpp"
                }
            ], 
            "name": "vector_addition"
        }
    ], 
    "name": "Error Handling (CPP)"
}