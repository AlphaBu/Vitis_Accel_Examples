{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/dummy_kernel.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/dummy_kernel.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example illustrates several ways to use the OpenCL API to transfer data to and from the FPGA"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "data_transfer"
        }
    ], 
    "key_concepts": [
        "OpenCL API", 
        "Data Transfer", 
        "Write Buffers", 
        "Read Buffers", 
        "Map Buffers", 
        "Async Memcpy"
    ], 
    "keywords": [
        "enqueueWriteBuffer()", 
        "enqueueReadBuffer()", 
        "enqueueMapBuffer()", 
        "enqueueUnmapMemObject()", 
        "enqueueMigrateMemObjects()"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "dummy_kernel", 
                    "location": "src/dummy_kernel.cpp"
                }
            ], 
            "name": "dummy_kernel"
        }
    ], 
    "name": "Data Transfer (C)"
}