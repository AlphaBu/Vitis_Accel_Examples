    "host": [
        {
            "sources": [
                "src/host.cpp"
            ], 
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ], 
                    "options": [
                        "-pthread"
                    ]
                }
            ], 
            "host_exe": "vadd_stream"
        }
    ], 
    "key_concepts": [
        "Read/Write Stream", 
        "Create/Release Stream"
    ], 
