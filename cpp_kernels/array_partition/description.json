{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to achieve better performance by array partitioning, using HLS kernel in SDx Environment."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "array_partition"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "HLS C Kernel", 
        "Array Partition"
    ], 
    "keywords": [
        "#pragma HLS ARRAY_PARTITION", 
        "complete"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "matmul", 
                    "location": "src/matmul.cpp"
                }, 
                {
                    "name": "matmul_partition", 
                    "location": "src/matmul_partition.cpp"
                }
            ], 
            "name": "matmul"
        }
    ], 
    "name": "Array Partition (HLS C/C++ Kernel)"
}