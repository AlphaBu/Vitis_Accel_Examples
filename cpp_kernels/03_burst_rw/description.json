{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to achieve better pipeline with burst read and write to/from local memory from/to DDR."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Burst Read/Write"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "src/mmult.cpp", 
                    "clflags": "--xp param:compiler.axiDeadLockFree=no", 
                    "name": "mmult"
                }
            ], 
            "name": "mmult"
        }
    ], 
    "name": "03 Matrix Multiplication Burst Read Write"
}