{
    "limitation": "Currently we ONLY support array size smaller than 512*512 due to some issue in the tool", 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/window_array_2d.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/window_array_2d.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of accessing each window of data from 2d array"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "window of 2D data array access"
    ], 
    "keywords": [
        "#pragma HLS DATAFLOW", 
        "#pragma HLS PIPELINE", 
        "#pragma HLS stream"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "window_array_2d", 
                    "location": "src/window_array_2d.cpp"
                }
            ], 
            "name": "window_array_2d"
        }
    ], 
    "name": "Read/Write Window of 2D Array (C)"
}