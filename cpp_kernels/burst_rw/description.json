{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is simple example of using AXI4-master interface for burst read and write"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2", 
                        "REPO_DIR/common/includes/bitmap"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2", 
                        "bitmap"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "burst access"
    ], 
    "keywords": [
        "memcpy", 
        "max_read_burst_length", 
        "max_write_burst_length"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vadd", 
                    "location": "src/vadd.cpp"
                }
            ], 
            "name": "vadd"
        }
    ], 
    "name": "Burst Read/Write (C)"
}