        "OpenCL APIs"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "clflags": "--xp param:compiler.enableAutoPipelining=false", 
                    "location": "src/mmult.cpp", 
                    "name": "mmult"
                }
            ], 
            "name": "mmult"
        }
    ], 
    "name": "01 Matrix Multiplication with OpenCL Kernel"
