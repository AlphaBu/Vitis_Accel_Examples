{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of accessing full data from 2d array"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "2D data full array Access"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "mmult", 
                    "location": "src/mmult.cpp"
                }
            ], 
            "name": "mmult"
        }
    ], 
    "name": "Full 2D Array Read/Write (C)"
}