{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to achieve better pipeline II factor by loop reordering."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Loop reorder to improve II"
    ], 
    "keywords": [
        "#pragma HLS PIPELINE", 
        "#pragma HLS ARRAY_PARTITION"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "src/mmult.cpp", 
                    "clflags": "--xp prop:solution.hls_pre_tcl=data/hls_config.tcl", 
                    "name": "mmult"
                }
            ], 
            "name": "mmult"
        }
    ], 
    "name": "Loop Reorder for better Performance(C)"
}