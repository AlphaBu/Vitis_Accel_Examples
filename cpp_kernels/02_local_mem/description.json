{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to reduce number of memory accesses using local memory."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Local Memory"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "clflags": "--xp param:compiler.enableAutoPipelining=false", 
                    "location": "src/mmult.cpp", 
                    "name": "mmult"
                }
            ], 
            "name": "mmult"
        }
    ], 
    "name": "02 Matrix Multiplication with Local Memory"
}