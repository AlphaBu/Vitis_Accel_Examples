{
    "config_make": "config.mk", 
    "testinfo": {
        "level_up": [
            {
                "hw": "1"
            }
        ]
    }, 
    "ndevice": [
        "xilinx_aws-vu9p-f1-04261818", 
        "xilinx:aws-vu9p-f1-04261818:dynamic"
    ], 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/krnl_kernel_global.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/krnl_kernel_global.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "Bandwidth test of global to local memory."
    ], 
    "host": [
        {
            "sources": [
                "src/kernel_global_bandwidth.cpp"
            ], 
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "kernel_global"
        }
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "max_memory_ports": "true", 
                    "name": "bandwidth", 
                    "location": "src/kernel.cl"
                }
            ], 
            "name": "krnl_kernel_global"
        }
    ], 
    "name": "kernel_global"
}