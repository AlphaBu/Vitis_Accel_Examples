{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/N_stage_Adders.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/N_stage_Adders.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is simple example of Multiple Stages Vector Addition to demonstrate Array of Stream usage in HLS C Kernel Code."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Array of Stream"
    ], 
    "keywords": [
        "dataflow", 
        "hls::stream<>"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "clflags": "--xp param:compiler.axiDeadLockFree=yes", 
                    "location": "src/N_stage_adders.cpp", 
                    "name": "N_stage_Adders"
                }
            ], 
            "ldclflags": "--xp param:compiler.axiDeadLockFree=yes", 
            "name": "N_stage_Adders"
        }
    ], 
    "name": "Dataflow Using HLS Stream (C)"
}