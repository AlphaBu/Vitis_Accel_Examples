{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vector_addition.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example demonstrates how loop pipelining can be used to improve the performance of a kernel."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "loop_pipeline"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Loop Pipelining"
    ], 
    "keywords": [
        "pragma HLS PIPELINE"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "vadd", 
                    "location": "src/vector_addition_BAD.cpp"
                }, 
                {
                    "name": "vadd_pipelined", 
                    "location": "src/vector_addition.cpp"
                }
            ], 
            "name": "vector_addition"
        }
    ], 
    "name": "Loop Pipelining (C)"
}