{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/adder.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/adder.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This is simple example of vector addition to demonstrate Dataflow functionality of HLS. HLS Dataflow allows user to schedule multiple task together to achieve higher throughput."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Task Level Parallelism"
    ], 
    "keywords": [
        "dataflow", 
        "hls::stream<>"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "adder", 
                    "location": "src/adder.cpp"
                }
            ], 
            "name": "adder"
        }
    ], 
    "name": "Dataflow Using HLS Stream"
}