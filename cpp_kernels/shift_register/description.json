{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/fir.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/fir.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example demonstrates how to shift values in registers in each clock cycle"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Shift Register", 
        "FIR"
    ], 
    "keywords": [
        "#pragma HLS ARRAY_PARTITION"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "fir_naive", 
                    "location": "src/fir_naive.cpp"
                }, 
                {
                    "name": "fir_shift_register", 
                    "location": "src/fir_shift_register.cpp"
                }
            ], 
            "name": "fir"
        }
    ], 
    "name": "Shift Register (C)"
}