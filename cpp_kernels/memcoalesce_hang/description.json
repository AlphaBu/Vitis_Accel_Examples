{
    "testinfo": {
        "level_up": [
            {
                "hw": "1"
            }
        ]
    }, 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/kernel_GOOD.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/kernel_GOOD.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example shows Memory Coalesce Deadlock/Hand situation and how to handle it. User can switch between BAD and GOOD case using makefile variable KFLOW."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Memory Coalesce", 
        "Memory Deadlock/Hang", 
        "Multiple Interfaces"
    ], 
    "keywords": [
        "HLS INTERFACE", 
        "bundle", 
        "m_axi"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "clflags": "-DDEADLOCK=0", 
                    "location": "src/kernel.cpp", 
                    "name": "dummy_op"
                }
            ], 
            "name": "kernel_GOOD"
        }
    ], 
    "name": "Memory Coalesce Deadlock/Hang Handling (C)"
}