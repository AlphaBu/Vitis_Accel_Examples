{
    "config_make": "config.mk", 
    "testinfo": {
        "level_up": [
            {
                "hw": "1"
            }
        ]
    }, 
    "ndevice": [
        "xilinx_u280-es1_xdma", 
        "xilinx_u200_xdma", 
        "xilinx_u200_qdma", 
        "xilinx_u250_xdma", 
        "xilinx_u280_xdma", 
        "xilinx_u250_qdma", 
        "xilinx:u280-es1:xdma", 
        "xilinx:u200:xdma", 
        "xilinx:u200:qdma", 
        "xilinx:u250:xdma", 
        "xilinx:u280:xdma", 
        "xilinx:u250:qdma"
    ], 
    "name": "kernel_global for 5.0 shell", 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/krnl_kernel_global.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "platform_type": "pcie", 
    "host": {
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2", 
                "src/kernel_global_bandwidth.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }, 
        "host_exe": "kernel_global"
    }, 
    "runtime": [
        "OpenCL"
    ], 
    "os": [
        "Linux"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "max_memory_ports": "true", 
                    "name": "bandwidth", 
                    "location": "src/kernel.cl"
                }
            ], 
            "name": "krnl_kernel_global"
        }
    ], 
    "description": [
        "Bandwidth test of global to local memory."
    ]
}