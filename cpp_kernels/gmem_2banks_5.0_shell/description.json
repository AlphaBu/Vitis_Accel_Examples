    "platform_type": "pcie", 
    "host": {
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap"
            ]
        }, 
        "host_exe": "host"
    }, 
    "key_concepts": [
        "Multiple Banks"
    ], 
    "keywords": [
        "max_memory_ports", 
        "misc:map_connect", 
        "HLS Interface m_axi bundle"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "os": [
        "Linux"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "max_memory_ports": "true", 
                    "name": "apply_watermark", 
                    "location": "src/apply_watermark.cpp"
                }
            ], 
            "ldclflags": "--sp apply_watermark_1.m_axi_gmem0:bank0 --sp apply_watermark_1.m_axi_gmem1:bank1", 
            "name": "apply_watermark"
        }
    ], 
    "output_files": "output.bmp"
