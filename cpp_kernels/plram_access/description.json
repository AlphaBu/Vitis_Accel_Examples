{
    "ndevice": [
        "xilinx_aws-vu9p-f1-04261818", 
        "xilinx:aws-vu9p-f1-04261818:dynamic"
    ], 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example shows the usage of PLRAM and how to use it with simple matrix multiplication (Row x Col)."
    ], 
    "keywords": [
        "PLRAM"
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "SDx Memory Hierarchy", 
        "PLRAMs"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "mmult", 
                    "location": "src/mmult.cpp"
                }
            ], 
            "ldclflags": "--sp mmult_1.a:PLRAM[0] --sp mmult_1.b:DDR[0] --sp mmult_1.c:PLRAM[1]", 
            "name": "mmult"
        }
    ], 
    "name": "PLRAM access (C)"
}