{
    "ndevice": [
        "xilinx_aws-vu9p-f1-04261818", 
        "xilinx:aws-vu9p-f1-04261818:dynamic"
    ], 
    "name": "PLRAM access (C)", 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
