{
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/nearest_neighbor.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/nearest_neighbor.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "description": [
        "This example will demonstrate how to fuse two loops into one to improve the performance of an OpenCL  C/C++ Kernel."
    ], 
    "host": [
        {
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "loop_fusion"
        }
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Loop Fusion", 
        "Loop Pipelining"
    ], 
    "keywords": [
        "#pragma HLS PIPELINE"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "nearest_neighbor_loop_fusion", 
                    "location": "src/nearest_neighbor.cpp"
                }, 
                {
                    "name": "nearest_neighbor", 
                    "location": "src/nearest_neighbor_bad.cpp"
                }
            ], 
            "name": "nearest_neighbor"
        }
    ], 
    "name": "Loop Fusion (C)"
}