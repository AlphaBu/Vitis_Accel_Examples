{
    "perf_fields": [
        "Board", 
        "Image Size", 
        "Frames / Second"
    ], 
    "name": "Median Filter", 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "platform_type": "pcie", 
    "description": [
        "This is an optimized implementation of a median filter being used to remove noise in images targeting execution on an SDAccel supported FPGA acceleration card."
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/krnl_median.xclbin PROJECT/data/inputImage.bmp PROJECT/data/goldenImage.bmp", 
            "name": "generic launch for all flows"
        }
    ], 
    "xcl": false, 
    "opencv": true, 
    "host": {
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap", 
                "src/medianFilter.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap"
            ]
        }, 
        "host_exe": "median"
    }, 
    "performance": [
        [
            "xilinx:adm-pcie-7v3:1ddr", 
            "128 x 128", 
            "22,222"
        ]
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "median", 
                    "location": "src/krnl_medianFilter.cl"
                }
            ], 
            "name": "krnl_median"
        }
    ], 
    "output_files": "output.bmp"
}