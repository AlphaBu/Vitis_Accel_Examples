{
    "perf_fields": [
        "Board", 
        "Image Size", 
        "Frames / Second"
    ], 
    "ndevice": [
        "xilinx_aws-vu9p-f1-04261818", 
        "xilinx:aws-vu9p-f1-04261818:dynamic"
    ], 
    "name": "Watermarking", 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/apply_watermark.xclbin PROJECT/data/inputImage.bmp PROJECT/data/golden.bmp", 
            "name": "generic launch for all flows"
        }
    ], 
    "description": [
        "This is an optimized implementation of a watermarking application to add watermarking to images targeting execution on an SDAccel supported FPGA acceleration card."
    ], 
    "platform_type": "pcie", 
    "host": {
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap", 
                "src/watermarking.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2", 
                "REPO_DIR/common/includes/bitmap"
            ]
        }, 
        "host_exe": "watermark"
    }, 
    "performance": [
        [
            "xilinx:adm-pcie-ku3:2ddr-xpr", 
            "1024 x 768", 
            "2,658"
        ], 
        [
            "xilinx:xil-accel-rd-ku115:4ddr-xpr", 
            "1024 x 768", 
            "3526"
        ]
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "src/krnl_watermarking.cl", 
                    "name": "apply_watermark", 
                    "clflags": "--config apply_watermark_apply_watermark.ini",
		    "compute_units": [
			{
				"arguments": [
					{
						"name": "input",
						"memory": "DDR[0]"
					},
					{
						"name": "output",
						"memory": "DDR[1]"
					}
				]
			}
			]		
					
                }
            ], 
            "name": "apply_watermark"
        }
    ], 
    "output_files": "output.bmp"
}
