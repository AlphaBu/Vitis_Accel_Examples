{
    "config_make": "config.mk", 
    "description": [
        "This is a simple Vector Increment RTL Kernel to Kernel design with 1 Stream input and 1 Stream output that demonstrates on how to process an input stream of data for computation in an application."
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "host": {
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2", 
                "src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }, 
        "linker": {
            "options": [
                "-pthread"
            ]
        }, 
        "host_exe": "vadd_stream"
    }, 
    "key_concepts": [
        "Read/Write Stream", 
        "Create/Release Stream", 
        "RTL Kernel"
    ], 
    "keywords": [
        "cl_stream", 
        "CL_STREAM_EOT"
    ], 
    "targets": [
        "hw", 
        "hw_emu"
    ], 
    "output_files": "tmp_kernel_pack* packaged_kernel* _x* *_ex project* pinfo.json *.xo", 
    "ndevice": [
        "xilinx_u200_xdma", 
        "zc", 
        "xilinx_u250_xdma", 
        "xilinx_u280_xdma", 
        "xilinx_u280-es1_xdma", 
        "xilinx_aws-vu9p-f1-04261818", 
        "xilinx_v350-es1_xdma",
	"xilinx:v350-es1:xdma",
	"xilinx:u200:xdma", 
        "xilinx:u250:xdma", 
        "xilinx:u280:xdma", 
        "xilinx:u280-es1:xdma", 
        "xilinx:aws-vu9p-f1-04261818:dynamic"
    ], 
    "name": "Stream Kernel to Kernel Vector Increment (RTL)", 
    "launch": [
        {
            "cmd_args": "BUILD/myadder.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "gui": false, 
    "platform_type": "pcie", 
    "runtime": [
        "OpenCL"
    ], 
    "os": [
        "Linux"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "kernel_type": "RTL", 
                    "name": "myadder1"
                }, 
                {
                    "kernel_type": "RTL", 
                    "name": "myadder2"
                }
            ], 
            "name": "myadder"
        }
    ]
}
