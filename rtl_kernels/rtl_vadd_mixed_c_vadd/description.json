{
    "config_make": "config.mk", 
    "os": [
        "Linux"
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "sw_emu"
        }, 
        {
            "cmd_args": "BUILD/vadd.xclbin", 
            "name": "hw_emu"
        }
    ], 
    "output_files": "tmp_kernel_pack* packaged_kernel*", 
    "gui": "false", 
    "description": [
        "This example has one RTL kernel and one C kernel. Both RTL kernel and C kernel perform vector addition. The C kernel reads the output from RTL kernel as one of two inputs."
    ], 
    "name": "Mixed Kernels (RTL & C)", 
    "host": [
        {
            "sources": [
                "src/host.cpp"
            ], 
            "compiler": [
                {
                    "includepaths": [
                        "REPO_DIR/common/includes/xcl2"
                    ]
                }
            ], 
            "linker": [
                {
                    "libraries": [
                        "xcl2"
                    ], 
                    "librarypaths": [
                        "REPO_DIR/common/includes/"
                    ]
                }
            ], 
            "host_exe": "host"
        }
    ], 
    "key_concepts": [
        "Mixed Kernels"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "targets": [
        "hw", 
        "hw_emu"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "kernel_type": "RTL", 
                    "name": "rtl_vadd"
                }, 
                {
                    "name": "krnl_vadd", 
                    "location": "src/krnl_vadd.cpp"
                }
            ], 
            "name": "vadd"
        }
    ], 
    "revision": [
        {
            "date": "July2017", 
            "version": "1.0", 
            "description": "Initial Xilinx Release"
        }
    ]
}